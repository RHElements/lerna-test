{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Link list",
    "description": "This element renders a styled list of links.",
    "type": "object",
    "tag": "pfe-link-list",
    "class": "pfe-link-list",
    "category": "content",
    "properties": {
      "slots": {
        "title": "Slots",
        "description": "Definition of the supported slots",
        "type": "object",
        "properties": {
          "content": {
            "title": "Content",
            "type": "array",
            "namedSlot": false,
            "items": {
              "title": "Body item",
              "oneOf": [
                {
                  "$ref": "raw"
                }
              ]
            }
          }
        }
      },
      "attributes": {
        "title": "Attributes",
        "type": "object",
        "properties": {}
      }
    },
    "required": ["slots", "attributes"],
    "additionalProperties": false
  }
