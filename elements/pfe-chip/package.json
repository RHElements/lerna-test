{
  "name": "@patternfly/pfe-chip",
  "description": "Chip element for PatternFly Elements",
  "pfelement": {
    "className": "PfeChip",
    "elementName": "pfe-chip",
    "preview": "chip--screenshot.png",
    "files": {
      "styles": "./src/pfe-chip.scss",
      "template": "./src/pfe-chip.html",
      "js": "./src/pfe-chip.js"
    },
    "assets": []
  },
  "version": "1.7.0",
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements",
    "directory": "elements/pfe-chip"
  },
  "engines": {
    "node": ">=10 <13"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox >= 78",
    "iOS >= 8",
    "ie 11"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/pfe-chip.umd.js",
  "module": "./dist/pfe-chip.js",
  "unpkg": "./dist/pfe-chip.umd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/gulp",
    "postbuild": "../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "../../node_modules/.bin/gulp dev",
    "watch": "../../node_modules/.bin/gulp watch",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json elements/pfe-chip/test/"
  },
  "contributors": [
    {
      "name": "castastrophe"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@patternfly/pfelement": "^1.7.0",
    "@patternfly/pfe-badge": "^1.7.0",
    "@patternfly/pfe-icon": "^1.7.0"
  },
  "devDependencies": {
    "@patternfly/pfe-sass": "^1.7.0"
  }
}
