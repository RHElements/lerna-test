{
  "name": "@rhdc-fed/pfe-popover",
  "description": "Popover element for PatternFly Elements",
  "pfelement": {
    "className": "PfePopover",
    "elementName": "pfe-popover",
    "files": {
      "styles": "./src/pfe-popover.scss",
      "template": "./src/pfe-popover.html",
      "js": "./src/pfe-popover.js"
    },
    "assets": []
  },
  "version": "1.0.0-alpha.2",
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements",
    "directory": "elements/pfe-popover"
  },
  "engines": {
    "node": ">=10 <13"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox >= 78",
    "iOS >= 8",
    "ie 11"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/pfe-popover.umd.js",
  "module": "./dist/pfe-popover.js",
  "unpkg": "./dist/pfe-popover.umd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "../../node_modules/.bin/gulp dev",
    "watch": "../../node_modules/.bin/gulp watch",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json elements/pfe-popover/test/"
  },
  "contributors": [
    {
      "name": "heyMP"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@patternfly/pfelement": "^1.6.0",
    "@patternfly/pfe-icon": "^1.6.0",
    "@rhdc-fed/pfe-absolute-position": "^1.0.0-alpha.2",
    "gh-pages": "^2.2.0"
  },
  "devDependencies": {
    "@patternfly/pfe-sass": "^1.6.0"
  }
}
