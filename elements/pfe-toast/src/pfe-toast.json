{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Toast",
  "description": "This element renders slotted content in a toast alert.",
  "type": "object",
  "tag": "pfe-toast",
  "class": "pfe-toast",
  "category": "container",
  "properties": {
    "slots": {
      "title": "Slots",
      "description": "Definition of the supported slots",
      "type": "object",
      "properties": {
        "default": {
          "title": "Default",
          "type": "array",
          "namedSlot": false,
          "items": {
            "oneOf": [{
              "$ref": "raw" 
            }]
          }
        }
      }
    }
  },
  "required": ["slots"],
  "additionalProperties": false
}
